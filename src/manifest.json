{

    "manifest_version": 2,
    "name": "Bilibili Vup Stream Details",
    "version": "1.0.6",
  
    "description": "check bilibili vup stream details, data grabing from vup.darkflame.ga",
    "author": "Eric Lam",
    "developer": {
      "name": "Eric Lam",
      "url": "https://github.com/eric2788"
    },
    "icons": {
      "48": "icons/icon.png"
    },
    "permissions": [
      "*://vup.darkflame.ga/*",
      "webRequest"
    ],
    "content_scripts": [
      {
        "matches": ["*://live.bilibili.com/*"],
        "js": ["index.js"]
      }
    ]
  
  }